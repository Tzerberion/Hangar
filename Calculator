function calculate(input){

    let firstNumber = Number(input[0]);
    let secondNumber = Number(input[2]);
    let operator = input[1];
    
    if (operator === '+') {
        result = firstNumber + secondNumber;
    }
    else if (operator === '-') {
        result = firstNumber - secondNumber;
    }   
    else if (operator === '*') {
        result = firstNumber * secondNumber;
    }
    else {
        result = firstNumber / secondNumber;
    
    }
    
        console.log(`${firstNumber} ${operator} ${secondNumber} = ${result}`)


}

calculate(["5", "*" ,"4"])
